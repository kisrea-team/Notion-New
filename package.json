{
  "name": "nodrogen",
  "version": "1.2.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "cross-env NODE_OPTIONS='--inspect' PORT=3000 next dev",
    "dev:core": "cd nodrogen-core && cross-env NODE_OPTIONS='--inspect' PORT=3001 next dev",
    "build": "next build",
    "build:core": "cd nodrogen-core && npm run build && npx next start -p 3001",
    "start": "next start",
    "product": "next build && next start",
    "dmix": "npm-run-all -p dev:core dev",
    "mix": "npm-run-all -p build:core product",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "deploy": "vercel deploy",
    "preinstall": "npm install --package-lock-only --ignore-scripts && npx npm-force-resolutions"
  },
  "dependencies": {
    "@svgr/webpack": "^8.1.0",
    "@types/react-dom": "^18.2.18",
    "classnames": "^2.3.1",
    "katex": "^0.16.20",
    "ky": "^1.7.4",
    "memory-cache": "^0.2.0",
    "mermaid": "10.6.1",
    "next": "^15.1.4",
    "next-themes": "^0.2.1",
    "notion-client": "^7.1.6",
    "notion-types": "^7.1.6",
    "notion-utils": "^7.1.6",
    "npm-run-all": "^4.1.5",
    "p-memoize": "^7.1.1",
    "react": "^19.0.0-rc.1",
    "react-dom": "^19.0.0-rc.1",
    "react-icons": "^5.0.1",
    "react-loading-skeleton": "^3.5.0",
    "react-motion": "^0.5.2",
    "react-notion-x": "^7.2.6",
    "react-router-dom": "^6.23.1",
    "react-text-loop": "^2.3.0",
    "react-tweet-embed": "^2.0.0",
    "tailwindcss": "^3.4.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.1.0",
    "@types/node": "^16.11.2",
    "@types/prismjs": "^1.26.3",
    "@types/react": "18.2.30",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "postcss": "^8.4.31",
    "typescript": "^4.6.2"
  },
  "resolutions": {
    "@types/react": "17.0.44"
  }
}